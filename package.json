{
  "name": "@firstvetcom/nuxt-gtm",
  "version": "1.0.1",
  "description": "Nuxt module to add Google Tag Manager",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build && npm run client:build",
    "client:build": "nuxi generate client",
    "client:dev": "nuxi dev client --port 3300",
    "dev": "concurrently \"npm:client:dev\" \"npm:play:dev\"",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare client",
    "play:dev": "nuxi dev playground",
    "play:prod": "npm run prepack && nuxi dev playground",
    "release": "changelogen --release --push",
    "lint": "cross-env NODE_ENV=production eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@gtm-support/vue-gtm": "^2.2.0",
    "@nuxt/kit": "^4.2.2",
    "defu": "^6.1.4",
    "sirv": "^2.0.4"
  },
  "peerDependencies": {
    "nuxt": "^4.0.0"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.2.15",
    "@nuxt/devtools": "3.1.1",
    "@nuxt/devtools-ui-kit": "3.1.1",
    "@nuxt/eslint-config": "^1.12.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.2",
    "@nuxt/test-utils": "^3.21.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "changelogen": "^0.5.7",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "nuxt": "^4.2.2",
    "vitest": "^0.31.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/firstvetcom/nuxt-gtm.git"
  },
  "keywords": [
    "nuxt",
    "nuxt4",
    "module",
    "nuxt-module",
    "gtm",
    "google tag manager",
    "googletagmanager",
    "open-source"
  ],
  "homepage": "https://github.com/firstvetcom/nuxt-gtm#readme",
  "changelog": {
    "repo": {
      "provider": "github"
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "typescript",
        "webpack",
        "vue",
        "vite"
      ]
    }
  }
}